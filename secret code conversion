#include <stdio.h>

int main(void) {
    char s[1000],r[1000];
    int begin,count=0,end;
	// your code goes here
	printf("enter the string\n");
	gets(s);
	while(s[count]!='\0')
	
	count++;
	printf("%d",count);
	end=count-1;
	
	    for(begin=0;begin<count;begin++)
	    {
	        r[begin]=s[end];
	        end--;
	    }
	    
	printf("%s\n",r);
	return 0;
}

